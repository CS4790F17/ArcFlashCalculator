@model IEnumerable<UserIP>
@using ArcFlashCalculator.Models
@using MVCGrid.Web

@{
    ViewBag.Title = "User IP History";
    Layout = "~/Views/Shared/_AdminNavbarPartial.cshtml";
}

<div class="center-block">
    <h2>User IP History</h2>
</div>

<div class="panel panel-default">
    <div class="panel-body">

        @Html.Partial("_MVCGridToolbar", new MVCGridToolbarModel()
   {
       MVCGridName = "UserIP",
       PageSize = true,
       ColumnVisibility = true,
       Export = true,
       GlobalSearch = true
   })
          @Html.MVCGrid("UserIP") 
        </div>
</div>
@Html.ActionLink("Return to Reports Home", "ReportHome", "Admin", new { @class = "btn btn-primary" }, null)